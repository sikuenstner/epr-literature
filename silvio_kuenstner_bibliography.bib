@article{KalinContinuousWaveElectron2003,
  title = {The Continuous Wave Electron Paramagnetic Resonance Experiment Revisited},
  author = {Kälin, Moritz and Gromov, Igor and Schweiger, Arthur},
  date = {2003-02-01},
  journaltitle = {Journal of Magnetic Resonance},
  shortjournal = {Journal of Magnetic Resonance},
  volume = {160},
