  file     = {:files/Hoult-2000-The_principle_of_reciprocity_in_signal_strength_calculations.pdf:PDF},
  groups   = {NMR},
  keywords = {principle of reciprocity, signal strength, near‐field, far‐field},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/1099-0534%282000%2912%3A4%3C173%3A%3AAID-CMR1%3E3.0.CO%3B2-Q},
