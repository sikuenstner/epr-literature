@article{StonerDirectdetectedRapidscanEPR2004,
  title = {Direct-Detected Rapid-Scan {{EPR}} at 250 {{MHz}}},
  author = {Stoner, James W. and Szymanski, Dennis and Eaton, Sandra S. and Quine, Richard W. and Rinard, George A. and Eaton, Gareth R.},
  date = {2004-09},
  journaltitle = {Journal of Magnetic Resonance},
