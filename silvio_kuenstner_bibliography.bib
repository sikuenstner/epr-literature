  author = {Sun, Ruoheng and Jakes, Peter and Eurich, Svitlana and family=Holt, given=Désirée, prefix=van, useprefix=true and Yang, Shuo and Homberger, Melanie and Simon, Ulrich and Kungl, Hans and Eichel, Rüdiger-A.},
