@article{NikolantonakiMeasurementWhiteWines2019,
  title = {Measurement of White Wines Resistance against Oxidation by {{Electron Paramagnetic Resonance}} Spectroscopy},
  author = {Nikolantonaki, Maria and Coelho, Christian and Noret, Laurence and Zerbib, Marie and Vileno, Bertrand and Champion, Dominique and Gougeon, Régis D.},
