@inproceedings{WolfsonHandheldEPRScanner2015,
  title = {A Hand-Held {{EPR}} Scanner for Transcutaneous Oximetry},
  booktitle = {Medical {{Imaging}} 2015: {{Biomedical Applications}} in {{Molecular}}, {{Structural}}, and {{Functional Imaging}}},
  author = {Wolfson, Helen and Ahmad, Rizwan and Twig, Ygal and Blank, Aharon and Kuppusamy, Periannan},
