@article{DayanAdvancedSurfaceResonators2018,
  title = {Advanced Surface Resonators for Electron Spin Resonance of Single Microcrystals},
  author = {Dayan, Nir and Ishay, Yakir and Artzi, Yaron and Cristea, David and Reijerse, Edward and Kuppusamy, Periannan and Blank, Aharon},
