@article{RoesslerDirectAssignmentEPR2010,
  title = {Direct Assignment of {{EPR}} Spectra to Structurally Defined Iron-Sulfur Clusters in Complex {{I}} by Double Electron–Electron Resonance},
  author = {Roessler, Maxie M. and King, Martin S. and Robinson, Alan J. and Armstrong, Fraser A. and Harmer, Jeffrey and Hirst, Judy},
  date = {2010-02-02},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {PNAS},
  volume = {107},
  pages = {1930--1935},
  publisher = {{National Academy of Sciences}},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.0908050107},
  url = {https://www.pnas.org/content/107/5/1930},
  urldate = {2021-03-11},
  abstract = {In oxidative phosphorylation, complex I (NADH:quinone oxidoreductase) couples electron transfer to proton translocation across an energy-transducing membrane. Complex I contains a flavin mononucleotide to oxidize NADH, and an unusually long series of iron-sulfur (FeS) clusters, in several subunits, to transfer the electrons to quinone. Understanding coupled electron transfer in complex I requires a detailed knowledge of the properties of individual clusters and of the cluster ensemble, and so it requires the correlation of spectroscopic and structural data: This has proved a challenging task. EPR studies on complex I from Bos taurus have established that EPR signals N1b, N2 and N3 arise, respectively, from the 2Fe cluster in the 75 kDa subunit, and from 4Fe clusters in the PSST and 51 kDa subunits (positions 2, 7, and 1 along the seven-cluster chain extending from the flavin). The other clusters have either evaded detection or definitive signal assignments have not been established. Here, we combine double electron-electron resonance (DEER) spectroscopy on B. taurus complex I with the structure of the hydrophilic domain of Thermus thermophilus complex I. By considering the magnetic moments of the clusters and the orientation selectivity of the DEER experiment explicitly, signal N4 is assigned to the first 4Fe cluster in the TYKY subunit (position 5), and N5 to the all-cysteine ligated 4Fe cluster in the 75 kDa subunit (position 3). The implications of our assignment for the mechanisms of electron transfer and energy transduction by complex I are discussed.},
  eprint = {20133838},
  eprinttype = {pmid},
  file = {/home/silvio/Dokumente/Arbeit/Nextcloud/Literature/zotero/files/Roessler_et_al-2010-Direct_assignment_of_EPR_spectra_to_structurally_defined_iron-sulfur_clusters.pdf;/home/silvio/Zotero/storage/6U79ZIKI/1930.html},
  keywords = {DEER,distance measurement,mitochondria,NADH:quinone oxidoreductase,pulsed EPR},
  langid = {english},
  number = {5}
}

