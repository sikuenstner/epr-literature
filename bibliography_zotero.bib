@article{TseitlinDeconvolutionSinusoidalRapid2011,
  title = {Deconvolution of Sinusoidal Rapid {{EPR}} Scans},
  author = {Tseitlin, Mark and Rinard, George A. and Quine, Richard W. and Eaton, Sandra S. and Eaton, Gareth R.},
