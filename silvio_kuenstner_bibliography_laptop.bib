@incollection{VillamenaChapterEPRSpin2017,
  title = {Chapter 5 - {{EPR Spin Trapping}}},
  shorttitle = {{{EPR Spin Trapping}}},
  booktitle = {Reactive {{Species Detection}} in {{Biology}}},
  author = {Villamena, Frederick A.},
  editor = {Villamena, Frederick A.},
  date = {2017-01-01},
  pages = {163--202},
  publisher = {Elsevier},
  location = {Boston, US},
  doi = {10.1016/B978-0-12-420017-3.00004-9},
  abstract = {The direct detection of free radicals is limited by their extremely short half-life in aqueous solution and this is likely to be even shorter in biological systems. However, this chapter describes how free radicals can be detected in biological systems with electron paramagnetic resonance (EPR) spectroscopy by the use of small diamagnetic probes, e.g., nitrone-based and hydroxylamines, and paramagnetic probes, e.g., nitroxides and trityl radicals. EPR is a powerful tool able to provide key information on the concentration of reactive species formed, the nature of the radical formed, the kinetics of radical formation, oxygen concentration, and more. This chapter describes the basis of EPR and also the chemistry and classification of spin trapping, the analytical technique used to detect and identify the nature and structure of transient free radicals. The kinetics and thermodynamics of spin trapping and adduct decay, which are the two key properties to be considered in using spin traps for radical detection, are described in detail along with the use of nitrones in the process. The biostability and cytotoxicity of spin traps are also discussed and the various routes for nitrone synthesis are detailed. The chapter also discusses how to interpret the EPR spectra to accurately provide information on the identity, molecular size, and nature of the radical species being trapped as well as their concentration. The chapter concludes with a detailed discussion of the applications of spin trapping and how to overcome challenges posed by the biological milieu and the detection of oxygen species.},
  isbn = {978-0-12-420017-3},
  langid = {english},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/villamena-2017-chapter_5_-_epr_spin_trapping.pdf}
}

@article{ViswanathanStudyMarginalOscillator1975,
  title = {A {{Study}} of {{Marginal Oscillator Behavior}}},
  author = {Viswanathan, T. Lakshmi and Viswanathan, T. R. and Sane, Krishna V.},
  date = {1975-03},
  journaltitle = {IEEE Transactions on Instrumentation and Measurement},
  volume = {24},
  number = {1},
  pages = {55--61},
  issn = {1557-9662},
  doi = {10.1109/TIM.1975.4314369},
  abstract = {In a magnetic resonance spectrometer, a marginal oscillator is used as a transducer to convert a minute change in the tank coil resistance into a change in the amplitude of oscillation. Thus one can define a conversion gain as the ratio of the fractional change in the amplitude of oscillation to the fractional change in the tank coil resistance. Since the change in the tank coil resistance is made periodic at a low frequency to facilitate phase-sensitive detection, a study of the transient behavior of the oscillator becomes important. In this paper, the conversion gain, bandwidth, and noise associated with the oscillator are studied as functions of the circuit parameters, utilizing a piecewise linear characteristic for the active device. Unlike the studies carried out by other authors making use of a polynomial expansion for the nonlinear characteristic, the piecewise linear characteristic has been chosen because it is more amenable to synthesis. The sensitivity of the amplitude of oscillation to the variation of the characteristics of the active device has also been evaluated, and the relationship between the stability requirement of the device characteristics and the conversion gain has been established.},
  eventtitle = {{{IEEE Transactions}} on {{Instrumentation}} and {{Measurement}}},
  keywords = {Active noise reduction,Bandwidth,Coils,Magnetic resonance,Oscillators,Phase detection,Phase frequency detector,Piecewise linear techniques,Spectroscopy,Transducers},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/viswanathan_et_al-1975-a_study_of_marginal_oscillator_behavior.pdf}
}

