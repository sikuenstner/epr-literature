@article{SchwenzerMembraneDevelopmentVanadium2011,
  title = {Membrane {{Development}} for {{Vanadium Redox Flow Batteries}}},
  author = {Schwenzer, Birgit and Zhang, Jianlu and Kim, Soowhan and Li, Liyu and Liu, Jun and Yang, Zhenguo},
  date = {2011},
