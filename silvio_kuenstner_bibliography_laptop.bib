  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/marko_et_al-2021-simulation_of_nitrogen_nuclear_spin_magnetization_of_liquid_solved_nitroxides.pdf;/home/sk/Zotero/storage/CCZXY62B/Marko et al. - 2021 - Simulation of nitrogen nuclear spin magnetization .pdf}
}

@article{MarshallDispersionAbsorptionDISPA1980,
