@article{PilbrowLineshapesFrequencySweptFieldSwept1984,
  title = {Lineshapes in {{Frequency-Swept}} and {{Field-Swept EPR}} for {{Spin}} 1/2*},
  author = {Pilbrow, J. R},
  date = {1984-06-15},
  journaltitle = {Journal of Magnetic Resonance (1969)},
  shortjournal = {Journal of Magnetic Resonance (1969)},
  volume = {58},
  number = {2},
  pages = {186--203},
  issn = {0022-2364},
  doi = {10.1016/0022-2364(84)90209-9},
  abstract = {Application of the well-known formula S(B, ν) = CηQ0 Vij2f([ν − ν0]2, σν) which describes the net absorption of microwave radiation between two states, i and j, leads to a number of valuable new insights for magnetic-field-swept EPR. Here C is a constant which depends on the experimental conditions, |Vij|2 the square of the transition matrix element, Q0 the unloaded cavity Q factor, η the sample filling factor, σv, the linewidth (in frequency units), and f the lineshape function. The above equation describes both frequency-swept EPR, where ν0 is fixed and ν is varied, and field-swept EPR detected at a constant microwave frequency (ν = νc where ν0 is the proper field-dependent variable. The Aasa and Vanngard 1/g factor (J. Magn. Reson. 19, 308 (1975)) is required whenever the above equation is written in terms of field variables as has been a common practice for field-swept EPR. Linewidth anisotropy in field-swept EPR is found to arise in some cases simply as a consequence of the g anisotropy; true tinewidth anisotropy is a property of σν. Asymmetric field-swept lineshapes are predicted for g-strain broadening; i.e., when σν, depends linearly upon the magnetic field. Implications of these ideas for powder spectral simulations are described.},
  langid = {english},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/pilbrow-1984-lineshapes_in_frequency-swept_and_field-swept_epr_for_spin_1-2.pdf}
}

@book{PilbrowTransitionIonElectron1990,
  title = {Transition {{Ion Electron Paramagnetic Resonance}}},
