@article{Oraa-PobletePreconditioningOperationMembraneless2023,
  title = {Preconditioning {{Operation}} of {{Membraneless Vanadium Micro Redox Flow Batteries}}},
  author = {Oraá-Poblete, Beatriz and Perez-Antolin, Daniel and Maurice, Ange A. and Palma, Jesus and Kjeang, Erik and Quintero, Alberto E.},
