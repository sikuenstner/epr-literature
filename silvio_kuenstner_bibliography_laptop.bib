@article{TenanFrictionCapillarySystems1998,
  title = {Friction in Capillary Systems},
  author = {Tenan, M. A. and Hackwood, S. and Beni, G.},
  date = {1998-06-04},
  journaltitle = {Journal of Applied Physics},
  volume = {53},
  number = {10},
