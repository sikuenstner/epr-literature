@article{AlipourImprovementMagneticResonance2021,
  title = {Improvement of Magnetic Resonance Imaging Using a Wireless Radiofrequency Resonator Array},
  author = {Alipour, Akbar and Seifert, Alan C. and Delman, Bradley N. and Robson, Philip M. and Shrivastava, Raj and Hof, Patrick R. and Adriany, Gregor and Fayad, Zahi A. and Balchandani, Priti},
