@article{BlankESRMicroscopyNanoscopy2006,
  title = {{{ESR Microscopy}} and {{Nanoscopy}} with “{{Induction}}” {{Detection}}},
  author = {Blank, Aharon and Freed, Jack H.},
  date = {2006-12},
  journaltitle = {Israel Journal of Chemistry},
  shortjournal = {Isr. J. Chem.},
  volume = {46},
  number = {4},
  pages = {423--438},
  issn = {00212148, 18695868},
  doi = {10.1560/IJC_46_4_423},
  abstract = {The current state-of-the-art in the fields of Nuclear Magnetic Resonance (NMR) and Electron Spin Resonance (ESR) micro-imaging is reviewed. Special attention is given to the uniqueness and the advantages of the conventional “induction” detection method with respect to other emerging sensitive magnetic resonance detection and imaging techniques. Following this, a theoretical description of the factors affecting the sensitivity and resolution in induction detection ESR is provided. Based on the theory, new approaches to substantially improve ESR capabilities, both at room temperature and at low cryogenic temperatures, are discussed. Representative results of experiments conducted at room temperature and at frequencies of 10–16 GHz show that with test samples, a sensitivity of \textasciitilde 107 electron spins and a resolution of \textasciitilde 3 microns are currently available. The results confirm the validity of the theoretical approach and confirm the value of striving for even higher frequencies and lower temperatures, in order to further improve the performance Finally, some of the current and potential applications of ESR microscopy and nanoscopy (involving imaging with a resolution of \textasciitilde 100 nm or better) are presented.},
  langid = {english},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/blank_freed-2006-esr_microscopy_and_nanoscopy_with_“induction”_detection.pdf}
}

