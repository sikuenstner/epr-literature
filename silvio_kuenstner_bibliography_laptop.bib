@article{MakinoSimplifyingQuantitativeMeasurement2022,
  title = {Simplifying Quantitative Measurement of Free Radical Species Using an {{X-band EPR}} Spectrometer},
  author = {Makino, Yusuke and Ueno, Megumi and Shoji, Yoshimi and Nyui, Minako and Nakanishi, Ikuo and Fukui, Koji and Matsumoto, Ken-ichiro},
  date = {2022-05},
  journaltitle = {Journal of Clinical Biochemistry and Nutrition},
  shortjournal = {J Clin Biochem Nutr},
  volume = {70},
