@article{HeMappingB1Field2002,
  title = {Mapping of the {{B1}} Field Distribution of a Surface Coil Resonator Using {{EPR}} Imaging},
  author = {He, Guanglong and Evalappan, Sathesh P. and Hirata, Hiroshi and Deng, Yuanmu and Petryakov, Sergey and Kuppusamy, Periannan and Zweier, Jay L.},
  date = {2002},
  journaltitle = {Magnetic Resonance in Medicine},
  volume = {48},
