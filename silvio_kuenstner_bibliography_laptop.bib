@article{GenctenEffectGaluminaPrecipitation2017,
  title = {Effect of α- and γ-Alumina on the Precipitation of Positive Electrolyte in Vanadium Redox Battery},
  author = {Gencten, Metin and Gursu, Hurmus and Sahin, Yucel},
  date = {2017-10-05},
