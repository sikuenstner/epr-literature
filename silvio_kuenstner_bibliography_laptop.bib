@article{AzumaMolecularCrystalStructures1994,
  title = {Molecular and {{Crystal Structures}} of {{Complexes}} of {{Stable Free Radical BDPA}} with {{Benzene}} and {{Acetone}}},
  author = {Azuma, Nagao and Ozawa, Takehiro and Yamauchi, Jun},
  date = {1994-01-01},
  journaltitle = {Bulletin of the Chemical Society of Japan},
  shortjournal = {BCSJ},
  volume = {67},
  number = {1},
  pages = {31--38},
