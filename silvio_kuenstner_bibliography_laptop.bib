@article{KoydemirSolventCompatibilityParylene2014,
  title = {Solvent {{Compatibility}} of {{Parylene C Film Layer}}},
  author = {Koydemir, Hatice Ceylan and Külah, Haluk and Özgen, Canan},
  date = {2014-04},
  journaltitle = {Journal of Microelectromechanical Systems},
  volume = {23},
  number = {2},
