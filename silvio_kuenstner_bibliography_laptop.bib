@article{MandalLimitedStabilityBDPA2020,
  title = {On the {{Limited Stability}} of {{BDPA Radicals}}},
  author = {Mandal, Sucharita and Sigurdsson, Snorri Th.},
  date = {2020},
  journaltitle = {Chemistry – A European Journal},
  volume = {26},
  number = {33},
  pages = {7486--7491},
  issn = {1521-3765},
  doi = {10.1002/chem.202001084},
  abstract = {1,3-Bis(diphenylene)-2-phenylallyl (BDPA)-based radicals are of interest as polarizing agents for dynamic nuclear polarization (DNP). For this purpose, a BDPA-nitroxide biradical, employing a phosphodiester linkage, was synthesized. Contrary to what is commonly assumed, BDPA-derived radicals were observed to have limited stability. Hence, the effects of various factors on the stability of BDPA radicals were investigated. Solvent polarity was found to play a significant role on degradation; a polar BDPA radical was observed to degrade faster in a non-polar solvent, whereas non-polar radicals were more unstable in polar solvents. The rate of decomposition was found to increase non-linearly with increasing radical concentration; a 2-fold increase in concentration led to a 3-fold increase in the rate of degradation. Collectively, these results indicate that the dimerization is a significant degradation pathway for BDPA radicals and indeed, a dimer of one BDPA radical was detected by mass spectrometry.},
  langid = {english},
  keywords = {BDPA,dynamic nuclear polarization,EPR spectroscopy,radical stability,radicals},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/mandal_sigurdsson-2020-on_the_limited_stability_of_bdpa_radicals.pdf}
}

