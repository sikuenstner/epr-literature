@article{ZgadzaiElectronSpinResonanceDipstick2018,
  title = {Electron-{{Spin-Resonance Dipstick}}},
  author = {Zgadzai, Oleg and Twig, Ygal and Wolfson, Helen and Ahmad, Rizwan and Kuppusamy, Periannan and Blank, Aharon},
  date = {2018-07-03},
  journaltitle = {Analytical Chemistry},
  shortjournal = {Anal. Chem.},
  volume = {90},
  number = {13},
  pages = {7830--7836},
  publisher = {American Chemical Society (ACS)},
  issn = {0003-2700},
  doi = {10.1021/acs.analchem.8b00917},
  abstract = {Electron spin resonance (ESR) is a powerful analytical technique used for the detection, quantification, and characterization of paramagnetic species ranging from stable organic free radicals and defects in crystals to gaseous oxygen. Traditionally, ESR requires the use of complex instrumentation, including a large magnet and a microwave resonator in which the sample is placed. Here, we present an alternative to the existing approach by inverting the typical measurement topology, namely placing the ESR magnet and resonator inside the sample rather than the other way around. This new development relies on a novel self-contained ESR sensor with a diameter of just 2 mm and length of 3.6 mm, which includes both a small permanent magnet assembly and a tiny (∼1 mm in size) resonator for spin excitation and detection at a frequency of ∼2.6 GHz. The spin sensitivity of the sensor has been measured to be ∼1011 spins/√Hz, and its concentration sensitivity is ∼0.1 mM, using reference samples with a measured volume of just ∼10 nL. Our new approach can be applied for monitoring the partial pressure of oxygen in vitro and in vivo through its paramagnetic interaction with another stable radical, as well as for simple online quantitative inspection of free radicals generated in reaction vessels and electrochemical cells via chemical processes.},
  langid = {english},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/zgadzai_et_al-2018-electron-spin-resonance_dipstick.pdf}
}

