@article{LagutaRapidScanESR2022,
  title = {Rapid Scan {{ESR}}: {{A}} Versatile Tool for the Spin Relaxation Studies at (Sub){{THz}} Frequencies},
  shorttitle = {Rapid Scan {{ESR}}},
  author = {Laguta, O. and Sojka, A. and Marko, A. and Neugebauer, P.},
