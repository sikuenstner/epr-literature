@article{Skyllas-KazacosModelingVanadiumIon2012,
  title = {Modeling of Vanadium Ion Diffusion across the Ion Exchange Membrane in the Vanadium Redox Battery},
  author = {Skyllas-Kazacos, Maria and Goh, Leesean},
  date = {2012-05-01},
  journaltitle = {Journal of Membrane Science},
  shortjournal = {Journal of Membrane Science},
  volume = {399--400},
  pages = {43--48},
  issn = {0376-7388},
  doi = {10.1016/j.memsci.2012.01.024},
  abstract = {Studies have shown that the different vanadium ions in the all-vanadium redox battery (VRB) exhibit unequal rates of diffusion across the ion exchange membrane that gives rise to capacity loss due to a build up of vanadium ions in one half-cell with a corresponding decrease in the other. In this paper concentration profiles of the different vanadium ions have been modeled as a function of time under different charge and discharge currents in order to predict expected capacity loss over extended charge–discharge cycling. In the case of cation exchange membranes, a build up in total vanadium ion concentration in the positive half-cell is predicted, in agreement with observed trends in laboratory prototype VRB systems. Effects of factors other than diffusion are discussed in detail, along with electrolyte maintenance strategies to restore capacity during long-term VRB operation.},
  keywords = {Capacity loss,Modeling concentration gradients,Vanadium ion diffusion,Vanadium redox flow battery},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/skyllas-kazacos_goh-2012-modeling_of_vanadium_ion_diffusion_across_the_ion_exchange_membrane_in_the.pdf}
