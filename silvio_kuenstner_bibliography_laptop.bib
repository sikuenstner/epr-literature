@article{OttavianiElectronParamagneticResonance2001,
  title = {Electron {{Paramagnetic Resonance Investigations}} of {{Free Radicals}} in {{Extra Virgin Olive Oils}}},
  author = {Ottaviani, M. Francesca and Spallaci, Monia and Cangiotti, Michela and Bacchiocca, Mara and Ninfali, Paolino},
  date = {2001-08-01},
  journaltitle = {Journal of Agricultural and Food Chemistry},
  shortjournal = {J. Agric. Food Chem.},
  volume = {49},
  number = {8},
