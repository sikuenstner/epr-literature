@article{LiuDegradationEdibleOil2018,
  title = {Degradation of {{Edible Oil During Deep-Frying Process}} by {{Electron Spin Resonance Spectroscopy}} and {{Physicochemical Appreciation}}},
  author = {Liu, Ying and Wang, Yuanpeng and Cao, Peirang and Liu, Yuanfa},
  date = {2018},
  journaltitle = {European Journal of Lipid Science and Technology},
  volume = {120},
  number = {2},
  pages = {1700376},
  issn = {1438-9312},
  doi = {10.1002/ejlt.201700376},
  abstract = {During frying process of edible oil, lipid oxidation occurs, which is a complex process and involves free radical chain reactions. The impacts of oil with different fatty acid composition on free radicals were evaluated. An ESR study was performed to identify and quantify the formed radicals, along with the assessment of physicochemical parameters including peroxide value, oxidative stability, fatty acid composition, and volatile profile. Results showed an increase of formed free radicals in frying oils over frying time. Besides, frying oils with higher content of unsaturated fatty acids were more prone to be oxidized, as well as physicochemical parameters evidencing this phenomenon. Volatile compounds produced by β-scission homolytic cleavage of peroxide group during thermal oxidation in frying oils were detected by GC-MS/MS. Results implied major aldehyde volatile compounds were derived from hydroperoxide and oil with higher proportions of unsaturated fatty acids are more likely to produce volatile oxidation products. In order to compare the degree of lipid oxidation in deep-fried sunflower oil (high degree of unsaturation) and palm oil (high degree of saturation), the formed free radicals based on ESR, along with the assessment of physicochemical parameters including peroxide value, oxidative stability, fatty acid composition, and volatile profile to improve comprehensive assessment of degradation in oil with different fatty acid composition are investigated.},
  langid = {english},
  keywords = {electron spin resonance,free radical,frying oil,lipid oxidation},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/liu_et_al-2018-degradation_of_edible_oil_during_deep-frying_process_by_electron_spin_resonance.pdf}
}

@article{LiUnravelingRedoxCouples2018,
  title = {Unraveling the {{Redox Couples}} of {{VIII}}/{{VIV Mixed-Valent Na3V2}}({{PO4}}){{2O1}}.{{6F1}}.4 {{Cathode}} by {{Parallel-Mode EPR}} and {{In Situ}}/{{Ex Situ NMR}}},
  author = {Li, Chao and Shen, Ming and Lou, Xiaobing and Hu, Bingwen},
  date = {2018-12-06},
  journaltitle = {The Journal of Physical Chemistry C},
  shortjournal = {J. Phys. Chem. C},
  volume = {122},
  number = {48},
  pages = {27224--27232},
  publisher = {American Chemical Society},
  issn = {1932-7447},
  doi = {10.1021/acs.jpcc.8b09151},
  abstract = {We herein present a study on the sodiation/desodiation behavior of Na3V2(PO4)2O1.6F1.4 cathode with I4/mmm tetragonal space group by the combination of dual-mode electron paramagnetic resonance (EPR) and in situ/ex situ solid-state nuclear magnetic resonance (SSNMR) techniques. The complementary parallel/perpendicular mode EPR, in situ 23Na NMR, 23Na/51V MAS NMR, and two-dimensional 23Na MQMAS NMR experiments substantiate that (i) both VIV and VIII are electrochemically active in this material, with the former transforming to VV, whereas the latter transforming to VIV; (ii) VIV continually converts to VV during the whole charge process, whereas VIII mostly reacts at the higher-potential charge plateau region; (iii) the Na+ extraction/intercalation process of Na3V2(PO4)2O1.6F1.4 is a widespread solid-solution mechanism; (iv) electronic spin hopping might occur locally between VIII and VV ions generated during the electrochemical reaction, resulting in the inconsecutive shift in 23Na resonance. This study shows how EPR spectroscopy in combination with SSNMR can be applied to probe electronic structure and long/short-range structure evolutions of vanadium-based cathode materials upon electrochemical process.},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/li_et_al-2018-unraveling_the_redox_couples_of_viii-viv_mixed-valent_na3v2(po4)2o1.pdf}
}

@article{LiuRapidDetectionPositive2017,
  title = {Rapid Detection of the Positive Side Reactions in Vanadium Flow Batteries},
  author = {Liu, Le and Li, Zhaohua and Xi, Jingyu and Zhou, Haipeng and Wu, Zenghua and Qiu, Xinping},
  date = {2017-01-01},
  journaltitle = {Applied Energy},
  shortjournal = {Applied Energy},
  volume = {185},
  pages = {452--462},
  issn = {0306-2619},
  doi = {10.1016/j.apenergy.2016.10.141},
  abstract = {We present an optical detection method for rapid measurement of the positive side reactions in vanadium flow batteries (VFB). By measuring the transmittance of the positive electrolytes in VFB, the states of charge (SOC) of the positive electrolytes can be detected at very high resolution (better than 0.002\% in the SOC range from 98\% to 100\%), due to the nonlinear transmittance spectra caused by the interactions between V(IV) and V(V) ions. The intensity of the positive side reactions of a VFB can be rapidly measured by a few steps, attributing to the fact that the positive side reactions occur only during the high voltage charging process. The ratios of the positive side reactions at different charge currents and different flow rates are obtained while causing no damage to the battery. This optical detection method can rapidly determine the optimal parameters of the VFB system, providing new means for studying the electrochemical reactions in the VFB system and rapid test in industrial production of VFBs.},
  langid = {english},
  keywords = {Optical detection,Side reaction,State of charge monitoring,Transmittance,Vanadium flow battery},
  file = {/home/sk/Dokumente/Arbeit/box/Literature/zotero/files/liu_et_al-2017-rapid_detection_of_the_positive_side_reactions_in_vanadium_flow_batteries.pdf}
}

@article{LiuStateChargeMonitoring2012,
  title = {State of Charge Monitoring for Vanadium Redox Flow Batteries by the Transmission Spectra of {{V}}({{IV}})/{{V}}({{V}}) Electrolytes},
  author = {Liu, Le and Xi, Jingyu and Wu, Zenghua and Zhang, Wenguang and Zhou, Haipeng and Li, Weibin and Qiu, Xinping},
