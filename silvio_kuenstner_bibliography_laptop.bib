  shorttitle = {Spins at Work},
  author = {Kulikov, Ilia and Panjwani, Naitik A. and Vereshchagin, Anatoliy A. and Spallek, Domenik and Lukianov, Daniil A. and Alekseeva, Elena V. and Levin, Oleg V. and Behrends, Jan},
